{
    "name": "fullstack-vue-graphql-client",
    "version": 2,
    "builds": [
      {
        "src": "dist/**",
        "use": "@now/static"
      }
    ],
    "routes": [
      {
        "src": "/(js|css|img)/(.*)",
        "dest": "/dist/$1/$2"
      },
      {
        "src": "/favicon.ico",
        "dest": "/dist/favicon.ico"
      },
      {
        "src": "/(.*)",
        "dest": "/dist"
      }
    ],
    "alias": "vue-share-client",
    "build":{
      "env": {
        "graphql": "@graphql"
      }
    },
    "env": {
      "graphql": "@graphql"
    }
  }
 